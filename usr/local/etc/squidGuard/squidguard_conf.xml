<?xml version="1.0"?>
<squidGuard>
	<logdir>/var/squidGuard/log</logdir>
	<dbhome>/var/db/squidGuard</dbhome>
	<binpath>/usr/local/bin</binpath>
	<workdir>/usr/local/etc/squidGuard</workdir>
	<sgxml_file>/usr/local/etc/squidGuard/squidguard_conf.xml</sgxml_file>
	<enabled>on</enabled>
	<blacklist_enabled>on</blacklist_enabled>
	<blacklist_url>http://blacklist-nuclewall.rhcloud.com/nuclewall_blacklist.tar.gz</blacklist_url>
	<sources/>
	<acls/>
	<default>
		<name>default</name>
		<description/>
		<disabled/>
		<timename></timename>
		<redirect_mode>rmod_int</redirect_mode>
		<redirect></redirect>
		<rewritename></rewritename>
		<log></log>
		<notallowingip></notallowingip>
		<destname>all</destname>
	</default>
	<enablelog>on</enablelog>
	<enableguilog>off</enableguilog>
	<logrotation>on</logrotation>
	<current_lan_ip></current_lan_ip>
</squidGuard>